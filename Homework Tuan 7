#include <iostream>
using namespace std;

struct YearPopulation {
    int year;       // Năm
    int population; // Số người sinh ra
};

void nhapDuLieu(YearPopulation data[], int size) {
    for (int i = 0; i < size; i++) {
        cout << "Nhập năm: ";
        cin >> data[i].year;
        cout << "Nhập số lượng người sinh ra: ";
        cin >> data[i].population;
    }
}

void timNamCungDanSo(YearPopulation data[], int size) {
    cout << "Các năm có cùng số lượng người sinh ra:" << endl;
    for (int i = 0; i < size; i++) {
        for (int j = i + 1; j < size; j++) {
            if (data[i].population == data[j].population) {
                cout << data[i].year << " và " << data[j].year << " có cùng dân số: " << data[i].population << endl;
            }
        }
    }
}

void timDanSoLonNhatVaNhoNhat(YearPopulation data[], int size) {
    int maxPopulation = data[0].population;
    int minPopulation = data[0].population;
    int yearMax = data[0].year;
    int yearMin = data[0].year;

    for (int i = 1; i < size; i++) {
        if (data[i].population > maxPopulation) {
            maxPopulation = data[i].population;
            yearMax = data[i].year;
        }
        if (data[i].population < minPopulation) {
            minPopulation = data[i].population;
            yearMin = data[i].year;
        }
    }

    cout << "Năm có số người sinh ra nhiều nhất: " << yearMax << " với " << maxPopulation << " người." << endl;
    cout << "Năm có số người sinh ra ít nhất: " << yearMin << " với " << minPopulation << " người." << endl;
}

int main() {
    const int SIZE = 5; // Kích thước của danh sách (có thể thay đổi tùy thuộc vào dữ liệu)
    YearPopulation data[SIZE];

    // Nhập dữ liệu
    nhapDuLieu(data, SIZE);

    // Tìm các năm có cùng dân số
    timNamCungDanSo(data, SIZE);

    // Tìm năm có số dân sinh ra lớn nhất và nhỏ nhất
    timDanSoLonNhatVaNhoNhat(data, SIZE);

    return 0;
}
